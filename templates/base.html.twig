<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    <title>{% block title %} Sortir.com {% endblock %}</title>
    <link rel="stylesheet" href="{{ asset('css/app.css') }}">
    <link rel="stylesheet" href="{{ asset('css/nomalize.css') }}">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Rampart+One&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="{{ asset('image/logo-eni-ecole.png') }}">
    <link rel="icon" href="{{ asset('image/images.png') }}">
</head>
<body>
<div class="container">

    <header>
        <div class="logo">
            <div>
                <a href="{{ path('main_home') }}"><img alt="logo" src="{{asset('image/logo-sortir_2x-0087abb548-removebg-preview-removebg-preview.png')}}"></a>
            </div>
            <div class="link">{% include('inc/nav.html.twig') %}</div>

        </div>
    </header>


    <main>
        {% for label, messages in app.flashes %}
            {% for message in messages %}
                <div class="flash-{{ label }}">
                    {{ message }}
                </div>
            {% endfor %}
        {% endfor %}
        {% block body %} {% endblock %}
    </main>

    <footer>
        Â© {{ "now"|date("Y") }} Sorti.com - Projet ENI/2 - Nicolas Reymond/Jean-marc Vetu/Tatiana Pocquet
    </footer>
</div>
</body>
</html>